package study20260108;

public class JavaClass2 {

	public static void main(String[] args) {
		
		Movie movie = new Movie();
//		new Movie() 생성자 메서드
		
		System.out.println(movie.title);
		
		movie.title = "aaa";
		movie.dir = "bbb";
		movie.year = 2026;
		movie.limitAge = 12;
		
		System.out.println(movie.title);
		
		Movie movie2 = new Movie("ddd", 2020);
		
		System.out.println(movie2.title);
		
//		Music 클래스의 객체 생성하여
//		음악제목, 가수, 재생시간(총몇분) 저장되게하기
//		매개변수 있는 생성자 메서드로 데이터 저장하고
//		출력하기
		
		Music music = new Music("sing", "singer", "130");
		
		System.out.println(music.title + "\n" + music.artist + "\n" + music.playTime);
	}

}
