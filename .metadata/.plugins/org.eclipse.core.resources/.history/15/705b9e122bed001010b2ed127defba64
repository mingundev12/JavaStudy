package study20260109;

public class BingoBoard {
	private int[] board;
	int lines;

	BingoBoard() {
		board = new int[25];
		lines = 0;
		
		for(int i = 0; i < 25; i++) {
			board[i] = (int) (Math.floor(Math.random() * 100) + 1);
			
			for(int j = 0; j < i; j++) {
				if (board[j] == board[i]) {
					i--;
					break;
				}
			}
		}
	}
}
