package study20260107;

public class Move {

	public static void main(String[] args) {
		
		int num = 20;
		
		switch (num) {
		
		case 2 :
			System.out.println("2번 선택됨  실행!");
			break;
		case 4 :
			System.out.println("4번 선택됨!!!");
			break;
		case 20 :
			System.out.println("20번 선택인데....흠");
			break;
		case 54 :
			System.out.println("54번 선택인데?? 흠..");
			break;
		}

	}

}
