package study20260109;

public class TestAdv03 {

	public static void main(String[] args) {
		
//		문제 3
		int[] counselNum = { 12, 8, 15, 15, 3, 9, 20 };
		int[] contractNum = { 2, 1, 3, 0, 0, 1, 4 };
		int[] claimNum = { 0, 1, 0, 2, 1, 0, 3 };
		
		int counselWeek = 0, contractWeek = 0, claimWeek = 0;
		int[] conversionRate = new int[7], score = new int[7];
		int bestDay1 = 0, bestDay2 = 0;
		
		for (int i = 0; i < 7; i++) {
			counselWeek += counselNum[i];
			contractWeek += contractNum[i];
			claimWeek += claimNum[i];
		}
		
		System.out.println("주간 상담 총합 : " + counselWeek);
		System.out.println("주간 계약 총합 : " + contractWeek);
		System.out.println("주간 클레임 총합 : " + claimWeek);

	}

}
