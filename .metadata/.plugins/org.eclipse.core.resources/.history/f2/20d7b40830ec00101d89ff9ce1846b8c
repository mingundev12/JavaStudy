package study20260108;

public class TestMain {

	public static void main(String[] args) {
		// 배열 실습
		
		// 문1. 10개의 무작위 정수 값 저장하기(배열에)
		// 랜덤 범위는 1~50
		
		
		int[] array = new int[10];
		
		for (int i = 0; i < array.length; i++) {
			while(true) {
				array[i] = (int) Math.floor(Math.random() * 50) + 1;
				
				boolean dup = false;
				
				for (int j = 0; j < i; j++) {
					if (array[j] == array[i]) {
						dup = true;
						break;
					}
				}
				
				if(dup) break;
			}
		}
		
		for (int val : array) {
			System.out.print(val + " ");
		}
		System.out.println();

	}

}
